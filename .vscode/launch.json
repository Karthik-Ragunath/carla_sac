    "version": "0.2.0",
    "configurations": [
        {
            "name": "test:agent",
            "type": "python",
            "request": "launch",
            "program": "${workspaceFolder}/test_agent.py",
            "console": "integratedTerminal",
            "justMyCode": true,
            "cwd": "${workspaceFolder}",
            "args": [
            ]
        },
        {
            "name": "video_viz",
            "type": "python",
            "request": "launch",
            "program": "${workspaceFolder}/util_video_visualization.py",
            "console": "integratedTerminal",
            "justMyCode": true,
            "cwd": "${workspaceFolder}",
            "args": [
                "--image_dir", "visualization_inference_5/0",
                "--fps", "10",
                "--output_dir", "visualization_videos/reward_5"
            ]
        },
        {
            "name": "ppo_carla_racing_inference",
            "type": "python",
            "request": "launch",
            "program": "${workspaceFolder}/carla_ppo_inference.py",
            "console": "integratedTerminal",
            "justMyCode": true,
            "cwd": "${workspaceFolder}",
            "args": [
                // "--render",
                "--device_id", "0",
                "--img-stack", "20",
                "--log_seed", "101",
                "--context", "inference_101",
                "--num_steps_per_episode", "150",
                "--load_context", "inference_4"
            ]
        },
        {
            "name": "ppo_carla_racing",
            "type": "python",
            "request": "launch",
            "program": "${workspaceFolder}/carla_ppo_train.py",
            "console": "integratedTerminal",
            "justMyCode": true,
            "cwd": "${workspaceFolder}",
            "args": [
                // "--render",
                "--device_id", "0",
                "--img-stack", "20",
                "--log_seed", "103",
                "--action-repeat", "2",
                "--running_score", "15000",
                "--context", "train_103",
                "--load_context", "train_100",
                "--num_steps_per_episode", "150"
            ]
        },
        {
            "name": "ppo_carla_racing_imitation",
            "type": "python",
            "request": "launch",
            "program": "${workspaceFolder}/imitation_train.py",
            "console": "integratedTerminal",
            "justMyCode": true,
            "cwd": "${workspaceFolder}",
            "args": [
                "--device_id", "0",
                "--log_seed", "1",
                "--context", "imitation_1",
                "--load_context", "train_100",
                "--img-stack", "1"
            ]
        },
        {
            "name": "openai_racing",
            "type": "python",
            "request": "launch",
            "program": "${workspaceFolder}/openai_train.py",
            "console": "integratedTerminal",
            "justMyCode": true,
            "cwd": "${workspaceFolder}",
            "args": [
                "--load_recent_model",
                "--model_framework",
                "openai",
                "--train_context",
                "actor_critic_v5",
                "--mode",
                "train",
                "--device_id",
                "1"
            ]
        },
        {
            "name": "detect_bounding_box",
            "type": "python",
            "request": "launch",
            "program": "${workspaceFolder}/torch_base/detect_bounding_boxes.py",
            "console": "integratedTerminal",
            "justMyCode": true,
            "cwd": "${workspaceFolder}/",
            "args": [
                "--image_path",
                "test_carla_image.png",
                "--image_width",
                "300",
                "--image_height",
                "300"
            ]
        },
        {
            "name": "train",
            "type": "python",
            "request": "launch",
            "program": "${workspaceFolder}/train.py",
            "console": "integratedTerminal",
            "cwd": "${workspaceFolder}",
            "justMyCode": false,
            "args": [
                "--xparl_addr",
                "localhost:8080",
                "--framework",
                "torch",
                "--train_total_steps",
                "351100"
            ],
            "subProcess": true,
            "python": "${command:python.interpreterPath}"
        }
    ]
}